<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SpellKeyBoard">

        <!--鍵盤項目間距 預設10-->
        <attr name="KeyboardItemPadding" format="dimension" />
        <!--鍵盤的圖示 normal-->
        <attr name="normalKeyboardItem" format="reference" />
        <!--鍵盤的圖示 click-->
        <attr name="touchKeyboardItem" format="reference" />

        <!--鍵盤back的圖示 normal-->
        <attr name="KeyboardItemBackOne" format="reference" />
        <!--鍵盤back的圖示 click-->
        <attr name="KeyboardItemBackTwo" format="reference" />

        <!--顯示項目 左右間距 預設10-->
        <attr name="fillGridPaddingRL" format="dimension" />

        <!--答案的底圖 1-->
        <attr name="normalFillGridItemType1" format="reference" />
        <!--答案的底圖 1-->
        <attr name="normalFillGridItemType2" format="reference" />

        <!--作答結束[正確] 動畫延遲時間 預設2000毫秒-->
        <attr name="answerCorrectAnimateDelay" format="integer" />

    </declare-styleable>

</resources>